			<UpdateSelectStatus file="Kuaikan.ListViewCtrl.xml.lua" func="Item_UpdateSelectStatus" />
		</method_def>
		<event_def>
			<OnItemClick>
				<param>
				</param>
			</OnItemClick>
		</event_def>
		<objtemplate>
			<children>
				<obj id="client.bkg" class = "FillObject">
					<attr>
						<left>0</left>
						<top>0</top>
						<width>father.width</width>
						<height>father.height</height>
						<limitchild>1</limitchild>
						<filltype>Monochrome</filltype>
						<srccolor>394264</srccolor>
						<destcolor>394264</destcolor>
					</attr>
					<children>
						<obj id="client.thumbnail.layout" class="FillObject">
							<attr>
								<left>1</left>
								<top>1</top>
								<width>father.width-2</width>
								<height>father.height-2</height>
								<filltype>Monochrome</filltype>
								<srccolor>1F253D</srccolor>
								<destcolor>1F253D</destcolor>
							</attr>
							<children>
								<obj id="client.thumbnail" class="ImageObject">
									<attr>
										<left>0</left>
										<top>0</top>
										<width>father.width</width>
										<height>father.height</height>
										<drawmode>1</drawmode>
										<antialias>2</antialias>
									</attr>
								</obj>
							</children>
						</obj>
					</children>
					<eventlist>
						<event name="OnMouseEnter" file="Kuaikan.ListViewCtrl.xml.lua" func="Item_OnMouseEnter" />
						<event name="OnMouseLeave" file="Kuaikan.ListViewCtrl.xml.lua" func="Item_OnMouseLeave" />
						<event name="OnMouseWheel" file="Kuaikan.ListViewCtrl.xml.lua" func="Item_OnMouseWheel" />
						<event name="OnChar" file="Kuaikan.ListViewCtrl.xml.lua" func="Item_OnChar" />
						<event name="OnKeyDown" file="Kuaikan.ListViewCtrl.xml.lua" func="Item_OnKeyDown" />
						<event name="OnLButtonDown" file="Kuaikan.ListViewCtrl.xml.lua" func="Item_OnLButtonDown" />
					</eventlist>
				</obj>
			</children>
			<eventlist>
				<event name="OnDestroy" file="Kuaikan.ListViewCtrl.xml.lua" func="Item_OnDestroy" />
				<event name="OnInitControl" file="Kuaikan.ListViewCtrl.xml.lua" func="Item_OnInitControl"/>
			</eventlist>
		</objtemplate>
	</control>
	
