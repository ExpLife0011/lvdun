<xlue>
	<control class="Kuaikan.ListViewCtrlItem">
		<attr_def>

		</attr_def>
		<method_def>
			<Reset file="Kuaikan.ListViewCtrl.xml.lua" func="Item_Reset" />
			<SetData file="Kuaikan.ListViewCtrl.xml.lua" func="Item_SetData" />
			<Refresh file="Kuaikan.ListViewCtrl.xml.lua" func="Item_Refresh" />
			<UpdateBkg file="Kuaikan.ListViewCtrl.xml.lua" func="Item_UpdateBkg" />
			<UpdateThumnails file="Kuaikan.ListViewCtrl.xml.lua" func="Item_UpdateThumnails" />
			<SetDefaultThumbnail file="Kuaikan.ListViewCtrl.xml.lua" func="Item_SetDefaultThumbnail" />
