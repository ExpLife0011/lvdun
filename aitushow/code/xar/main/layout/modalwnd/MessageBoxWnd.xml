<xlue>
	<objtreetemplate id="MessageBoxWndTree" class="ObjectTreeTemplate">
		<attr>
			<left>0</left>
			<top>0</top>
			<width>430</width>
			<height>150</height>
		</attr>
		<obj id="root" class="MessageBox">
			<attr>
				<left>0</left>
				<top>0</top>
				<width>430</width>
				<height>150</height>
			</attr>	
		</obj>
	</objtreetemplate>

    <hostwndtemplate id="MessageBoxWnd" class="ModalHostWnd">
        <attr>
            <layered>1</layered>
            <left>0</left>
            <top>0</top>
            <width>430</width>
            <height>150</height>
            
            <topmost>0</topmost>
            <visible>1</visible>
            <enable>1</enable>
            <maxbox>1</maxbox>
            <minbox>1</minbox>
            <minwidth>50</minwidth>
            <minheight>50</minheight>
			<sysmenu>0</sysmenu>
			<appwindow>0</appwindow>
        </attr>
		 <eventlist>
            <event name="OnCreate" file="MessageBoxWnd.xml.lua" func="OnCreate"/>
            <event name="OnShowWindow" file="MessageBoxWnd.xml.lua" func="OnShowWindow"/>
        </eventlist>
    </hostwndtemplate>

</xlue>
