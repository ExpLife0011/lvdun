<xlue>
	<control class="BottomCtrl">
    <objtemplate>
		<children>
			<obj id="Background" class="TextureObject">
				<attr>
					<left>0</left>
					<top>0</top>
					<width>father.width</width>
					<height>father.height</height>
				</attr>
				<children>
					<obj id="Rotate.Left.Btn" class="TipAddin.Button">
						<attr>
							<left>19</left>
							<top>(father.height - 26)/2</top>
							<width>26</width>
							<height>26</height>
							<Bkg4In1ID>texture.bottomctrl.rotate.left</Bkg4In1ID>
						</attr>
						<eventlist>
							<event name="OnClick" file="BottomCtrl.xml.lua" func="OnClickRotateLeft"/>
						</eventlist>
					</obj>
					<obj id="Rotate.Right.Btn" class="TipAddin.Button">
						<attr>
							<left>19 + 26 + 19</left>
							<top>(father.height - 26)/2</top>
							<width>26</width>
							<height>26</height>
							<Bkg4In1ID>texture.bottomctrl.rotate.right</Bkg4In1ID>
						</attr>
						<eventlist>
							<event name="OnClick" file="BottomCtrl.xml.lua" func="OnClickRotateRight"/>
						</eventlist>
					</obj>
					<obj id="Delete.Btn" class="TipAddin.Button">
						<attr>
							<left>19 + 26 + 19 + 26 + 19</left>
							<top>(father.height - 26)/2</top>
							<width>26</width>
							<height>26</height>
							<Bkg4In1ID>texture.bottomctrl.delete</Bkg4In1ID>
						</attr>
						<eventlist>
							<event name="OnClick" file="BottomCtrl.xml.lua" func="OnClickDelete"/>
						</eventlist>
					</obj>
				</children>
			</obj>
		</children>
		<eventlist>
		</eventlist>
	</objtemplate>
	</control>
</xlue>